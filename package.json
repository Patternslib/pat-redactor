{
  "name": "pat-redactor",
  "version": "4.0.0",
  "title": "Patternslib plugin for redactor 3",
  "licences": [
    {
      "type": "BSD",
      "URL": "LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Patternslib/pat-redactor.git"
  },
  "keywords": [
    "patternslib",
    "pattern",
    "redactor"
  ],
  "dependencies": {
    "@patternslib/core": "https://github.com/patternslib/patternslib-core#95e4682d0647f62cd7c4c9b188b82b970cffb3dd"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "core-js": "3",
    "eslint": "^7.0.0",
    "jquery": "^3.5.0",
    "underscore": "^1.9.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --open --env.NODE_ENV=development",
    "build": "webpack --config webpack.config.js --env.NODE_ENV=production"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "homepage": "https://gitub.com/Patternslib/pat-redactor",
  "maintainers": [
    {
      "name": "Alexander Pilz",
      "email": "info@syslab.com",
      "url": "http://www.syslab.com"
    }
  ]
}
